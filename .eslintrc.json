{
    "root": true,
    "ignorePatterns": [
        "**/*"
    ],
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "no-unused-vars": "warn"
    }
}